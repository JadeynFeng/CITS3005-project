<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://uwabookofknowledge.org/onto.owl"
         xmlns="http://uwabookofknowledge.org/onto.owl#">

<owl:Ontology rdf:about="http://uwabookofknowledge.org/onto.owl"/>

<owl:ObjectProperty rdf:about="#has_unitcode">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Unit"/>
  <rdfs:range rdf:resource="#UnitCode"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_unittitle">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Unit"/>
  <rdfs:range rdf:resource="#UnitTitle"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#is_part_of_major">
  <rdfs:domain rdf:resource="#Unit"/>
  <rdfs:range rdf:resource="#Major"/>
  <owl:inverseOf rdf:resource="#contains_unit"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_majorcode">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Major"/>
  <rdfs:range rdf:resource="#MajorCode"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_majortitle">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Major"/>
  <rdfs:range rdf:resource="#MajorTitle"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#contains_unit">
  <rdfs:range rdf:resource="#Unit"/>
  <rdfs:domain rdf:resource="#Major"/>
  <owl:inverseOf rdf:resource="#is_part_of_major"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#has_unitlevel">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  <rdfs:domain rdf:resource="#Unit"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Unit">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#UnitCode">
  <rdfs:subClassOf rdf:resource="#Unit"/>
</owl:Class>

<owl:Class rdf:about="#UnitTitle">
  <rdfs:subClassOf rdf:resource="#Unit"/>
</owl:Class>

<owl:Class rdf:about="#Major">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#MajorCode">
  <rdfs:subClassOf rdf:resource="#Major"/>
</owl:Class>

<owl:Class rdf:about="#MajorTitle">
  <rdfs:subClassOf rdf:resource="#Major"/>
</owl:Class>

<owl:Class rdf:about="#Prereq">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#OrReq">
  <rdfs:subClassOf rdf:resource="#Prereq"/>
</owl:Class>

<owl:Class rdf:about="#Contact">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>


</rdf:RDF>
